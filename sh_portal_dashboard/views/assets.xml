<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="sh_portal_dashboard_assets_frontend" inherit_id="portal.assets_frontend" name="Portal Dashboard Assets" priority="15">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/sh_portal_dashboard/static/src/css/portal.css" />
            <link rel="stylesheet" type="text/css" href="/sh_portal_dashboard/static/src/css/chartist.css" />
            <link rel="stylesheet" type="text/css" href="/sh_portal_dashboard/static/src/css/chartist-plugin-tooltip.css" />
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/sh_portal_dashboard/static/src/js/jquery.cookie.js"></script>
            <script type="text/javascript" src="/sh_portal_dashboard/static/src/js/chartist.js"></script>
            <script type="text/javascript" src="/sh_portal_dashboard/static/src/js/chartist-plugin-tooltip.js"></script>
            <script type="text/javascript" src="/sh_portal_dashboard/static/src/js/chartist-plugin-axistitle.js"></script>
            <script type="text/javascript" src="/sh_portal_dashboard/static/src/js/sale_charts.js"></script>
        </xpath>
    </template>
</odoo>
